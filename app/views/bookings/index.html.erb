<h2>schedule</h2>

<table>
  <tr>
    <th>day</th>
    <th>course</th>
    <th>classroom</th>
  </tr>

<% @bookings.each do |booking| %>
  <tr>
    <td><%= booking.b_date %></td>
    
    
    <td><%= link_to booking.course.title, booking %></td>
    <td><%= booking.classroom.c_name %></td>
    
    
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Booking', new_booking_path %>
